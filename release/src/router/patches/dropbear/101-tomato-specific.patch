+
+#define MAX_UNAUTH_PER_IP 2  /* default 5 */
+#define MAX_UNAUTH_CLIENTS 5 /* default 30 */
+#define MAX_AUTH_TRIES 2     /* default 10 */
+#define AUTH_TIMEOUT 60      /* default 300 */
